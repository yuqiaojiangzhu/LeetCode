public class Solution {
    public int findLUSlength(String a, String b) {
        if(a==null && b==null){
            return -1;
        }else if(a==null && b!=null){
            return b.length();
        }else if(a!=null && b==null){
            return a.length();
        }
        
        if(a.equals(b)){
            return -1;
        }else{
            return Math.max(a.length(),b.length());
        }
        
    }
}
